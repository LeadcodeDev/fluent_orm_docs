    "@dimerapp/content": "^3.0.3",
    "@dimerapp/docs-theme": "^1.7.1",
    "@dimerapp/edge": "^3.0.1",
    "@docsearch/css": "^3.5.0",
    "@docsearch/js": "^3.5.0",
    "@swc/core": "^1.3.62",
    "@types/node": "^20.2.5",
    "alpinejs": "^3.12.2",
    "autoprefixer": "^10.4.15",
    "collect.js": "^4.36.1",
    "concurrently": "^8.1.0",
    "copyfiles": "^2.4.1",
    "edge-uikit": "^0.0.1-3",
    "medium-zoom": "^1.0.8",
