    "@adonisjs/application": "7.1.2-5"
  },
  "resolutions": {
    "edge.js": "6.0.0-1"
  },
  "dependencies": {
    "@adonisjs/core": "^6.1.5-4",
    "@adonisjs/static": "^1.0.1",
    "@adonisjs/view": "^7.0.0-2"
  }
